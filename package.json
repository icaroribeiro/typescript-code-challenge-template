{
  "name": "web-api-rest",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start:dev": "nodemon --config nodemon.json",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:prod": "npm run build && node dist/app.js",
    "typeorm": "node ./node_modules/typeorm/cli",
    "migration:create": "npm run typeorm migration:create ./src/database/migrations/$npm_config_name",
    "migration:create:win": "npm run typeorm migration:create ./src/database/migrations/%npm_config_name%",
    "lint": "eslint --config .eslintrc.json --ignore-path .eslintignore src/** tests/**",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --config .prettierrc --ignore-path .prettierignore src --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "test": "jest --config jest.config.js --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.8.0",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.1.0",
    "@types/better-sqlite3": "^7.6.5",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.17",
    "@types/faker": "^6.6.9",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.9",
    "better-sqlite3": "^8.7.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.0.2",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "factory.ts": "^1.4.1",
    "interface-forge": "^1.5.1",
    "jest": "^29.6.4",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "sinon": "^16.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typeorm-extension": "^3.0.2",
    "typeorm-faker": "^1.3.12",
    "typescript": "^5.2.2"
  }
}
